%html
  %head
    %title Pictures
    = csrf_meta_tags
    %meta{content: 'width=device-width,initial-scale=1', name: 'viewport'}/
    %meta{charset: 'utf-8'}/
    %meta{content: 'IE=edge,chrome=1', 'http-equiv': 'X-UA-Compatible'}/

    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
